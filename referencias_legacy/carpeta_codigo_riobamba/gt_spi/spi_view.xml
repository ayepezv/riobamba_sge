</record>

<record model="ir.ui.view" id="view_spi_form">
  <field name="name">Transferencias SPI</field>
  <field name="model">account.spi.voucher</field>
  <field name="type">form</field>
  <field name="arch" type="xml">         
    <form string="Transferencias SPI">
      <group  colspan="3">
      </group>
      <group  colspan="3">
	<field name="name" colspan="2" required="1" attrs="{'readonly': [('state', '=','Confirmado')]}"/>
	<field name="ref_int" colspan="2" readonly="1"/>
	<field name="ref" colspan="2" required="1" attrs="{'readonly': [('state', '=','Confirmado')]}"/>
	<field name="tipo" colspan="2" required="1" attrs="{'readonly': [('state', '=','Confirmado')]}"/>
	<field name="date_done" colspan="2" required="1" attrs="{'readonly': [('state', '=','Confirmado')]}"/>
	<field name="journal_id" colspan="2" required="1" domain="{('type','=','bank')}"/>
	<!--field name="date_aux" colspan="2" attrs="{'readonly': [('state', '=','Confirmado')]}"/-->
	<field name="quincena_id" colspan="4" attrs="{'invisible': [('tipo', '!=','quincena')],'required': [('tipo', '=','quincena')]}"/>
	<field name="payroll_id" colspan="4" domain="[('state','=','Autorizado')]"
	       attrs="{'invisible': [('tipo', '!=','mensual')],'required': [('tipo', '=','mensual')]}"/>
	<field name="varios_id" colspan="4" attrs="{'invisible': [('tipo', '!=','varios')],'required': [('tipo', '=','varios')]}" 
	       domain="[('state','=','pendiente')]"/>
	<field name="dec3_id" colspan="4" domain="[('state','=','Autorizado')]"
	       attrs="{'invisible': [('tipo', '!=','dec3')],'required': [('tipo', '=','dec3')]}"/>
	<field name="dec4_id" colspan="4" domain="[('state','=','Autorizado')]"
	       attrs="{'invisible': [('tipo', '!=','dec4')],'required': [('tipo', '=','dec4')]}"/>
      </group>
      <group  colspan="3" string="ZIP - Txt">
	<field name="file_name" invisible="1"/>
	<field name="archivo" filename="file_name"/>
	<newline/>
	<field name="file_namezp" invisible="1"/>
	<field name="archivozp" filename="file_namezp"/>
      </group>
      <group  colspan="4" string="EXCEL">
	<field name="file_namexls" invisible="1"/>
	<field name="archivoxls" filename="file_namexls" colspan="2"/>
	<button name="%(action_cambia_concepto_spi)d"
		string="Cambiar Concepto" type="action" icon="gtk-execute"/>
	<button name="%(action_no_move_spi)d" states='Borrador'
		string="No Carga Comprobante" type="action" icon="gtk-execute"/>
      </group>
      <notebook colspan="3">
	<page string="Detalle Pagos Generales" attrs="{'invisible': [('tipo', '!=','pgeneral')]}">
	  <field name="line2_ids" domain="[('state','=','posted'),('is_pay','=',False),('journal_id.type','in',('purchase','bank'))]" 
		 colspan="3" nolabel="1">
	    <tree editable="bottom">
	      <field name="name" string="Referencia" readonly="1"/>
	      <field name="partner_id" readonly="1"/>
	      <field name="amount" readonly="1" sum="Total Monto"/>
	      <field name="state" readonly="1"/>
	    </tree>
	  </field>
	</page>
	<page string="Detalle">
	  <field name="line_ids" domain="[('state','=','posted')]" colspan="3" 
		 nolabel="1">
	    <tree editable="bottom">
	      <field name="name" string="Referencia" readonly="1"/>
	      <field name="partner_id" readonly="1"/>
	      <field name="ref" readonly="1"/>
	      <field name="amount" sum="Total Monto"/>
	      <field name="state" invisible="1"/>
	      <button name="%(no_acreditado)d" states="Pagado" string="No Acreditado" type="action" icon="gtk-execute"/>
	    </tree>
	  </field>
	</page>
	<page string="Resumen">
	  <field name="resumen_ids" nolabel="1" readonly="1">
	    <tree>
	      <field name="number" readonly="1"/>
	      <field name="name" readonly="1"/>
	      <field name="qty" readonly="1"/>
	      <field name="amount" readonly="1" sum="Total Monto"/>
	    </tree>
	  </field>
	</page>
      </notebook>
      <group colspan="4" col="8">
	<!--field name="state" readonly="1"/-->
	<field name="state" widget="statusbar" statusbar_colors='{"Generado":"blue","Pagado":"green"}'/>
	<button name="spi_import_normal" groups="retention.group_tesorero" 
		states="Borrador" string="Importar Pagos" type="object" icon="gtk-ok"/>
	<!--button name="spi_import_normal" groups="retention.group_tesorero" 
		states="Borrador" string="Pagos Generales" type="object" icon="gtk-ok"/-->
	<button name="spi_realizado" groups="retention.group_tesorero"
		confirm="Si ejecuta esta accion los comprobantes ya no estaran disponibles para nuevos pagos" 
		states="Generado" string="Pago Realizado" type="object" icon="gtk-ok"/>
	<!--button name="spi_draft" groups="retention.group_tesorero" 
		states="Confirmado,Generado" string="Regresar Borrador" type="object" icon="gtk-go-back" 
		confirm="Confirmar desea regresar?"/-->
	<button name="spi_draft" groups="retention.group_tesorero" 
		states="Confirmado,Generado,Pagado" string="Regresar Borrador" type="object" icon="gtk-go-back" 
		confirm="Confirmar desea regresar?"/>
	<button name="ssp_generate" groups="retention.group_tesorero" 
		states="Borrador" string="Generar SSP" type="object" icon="gtk-execute" 
		confirm="Confirmar desea generar SSP?"/>
	<!--button name="spi_generate_xls" groups="retention.group_tesorero" 
		states="Borrador" string="Generar Excel SPI" type="object" icon="gtk-execute" 
		confirm="Confirmar desea generar SPI a excel?"/-->
	<button name="print_resumen" string="Imprimir Autorizacion" states="Generado" type="object" icon="gtk-print"/>
	<button name="print_resumen_" string="Imprimir Detalle" states="Generado" type="object" icon="gtk-print"/>
      </group>
    </form>
  </field>
</record>

<record model="ir.ui.view" id="view_spi_voucher_tree">
  <field name="name">account.spi.voucher.tree</field>
  <field name="type">tree</field>
  <field name="model">account.spi.voucher</field>
  <field name="arch" type="xml">
