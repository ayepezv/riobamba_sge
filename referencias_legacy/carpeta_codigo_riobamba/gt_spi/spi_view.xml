  <field name="arch" type="xml">
    <search string="Buscar Transferencias SPI">
      <group>
	<field name="spi_id" readonly="1"/>
	<field name="date" string="Fecha Desde" filter_domain="[('date','&gt;=',self)]"/>
	<field name="date" string="Fecha Hasta" filter_domain="[('date','&lt;=',self)]"/>
	<field name="partner_id" readonly="1"/>
	<field name="name" readonly="1"/>
	<field name="detalle" readonly="1"/>
	<field name="certificate_id" readonly="1"/>
	<field name="pago_id" readonly="1"/>
	<field name="bank_id" readonly="1"/>
	<field name="amount" readonly="1"/>
      </group>
      <newline/>
      <group expand="1" string="Agrupar por...">
        <filter string="Fecha" domain="[]" context="{'group_by':'date'}"/>
        <separator orientation="vertical"/>
	<filter string="Beneficiario" domain="[]" context="{'group_by': 'partner_id'}"/>
      </group>
    </search>
  </field>
</record>

