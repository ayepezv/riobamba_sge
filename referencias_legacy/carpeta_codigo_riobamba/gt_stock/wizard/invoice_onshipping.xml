<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

<!-- INVOICE LINE MODIFIED
-->

<record id="invoice_onshipping_modified" model="ir.ui.view">
  <field name="name">invoice.onshipping.modified</field>
  <field name="model">stock.invoice.onshipping</field>
  <field name="type">form</field>
  <field name="inherit_id" ref="stock.view_stock_invoice_onshipping"/>
  <field name="arch" type="xml">
    <field name="group" position="replace"></field>
    <field name="invoice_date" position="replace"></field>
    <field name="journal_id" position="replace">
      <field name="journal_id"/>
      <newline/>
      <field name="partner_id" readonly="1"/>
      <newline/>
      <field name="certificate_id" string="Compromiso Presupuestario" domain="[('state','=','commited'),('partner_id','=',partner_id)]"/>
    </field>
  </field>
</record>



    </data>
</openerp>

