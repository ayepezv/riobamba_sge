            usage="default" string="Reporte kdx" header="True" report_type="webkit"/>

    <record model="ir.ui.view" id="view_report_move_kdx_form">
      <field name="name">Movimientos Stock</field>
      <field name="model">report.move.head</field>
      <field name="type">form</field>
      <field name="arch" type="xml">         
        <form string="Movimientos">
          <group  colspan="4" col="6">
	    <field name="date_start" required="1"/>
	    <field name="date_end" required="1"/>
	    <newline/>
	    <field name="product_id"/>
	    <field name="qty_inicial" readonly="1"/>
	    <newline/>
	    <field name="bodega_id" required="1" colspan="4"/>
	    <newline/>
	  </group>
	    <button name="create_line_move_kdx" string="Generar" icon="gtk-execute" type="object"/>
	    <button name="print_line_move_kdx" string="Imprimir" icon="gtk-print" type="object"/>
	  <notebook colspan="4">
	    <page string="Movimientos">
	      <field name="line_ids" readonly="1" nolabel="1">
		<tree>
		  <field name="date"/>
		  <field name="department_id"/>
		  <field name="documento"/>
		  <field name="tipo"/>
		  <field name="qty"/>
		  <field name="saldo"/>
		  <field name="cu"/>
		  <field name="ct"/>
		</tree>
		<form>
		  <field name="date"/>
		  <field name="department_id"/>
		  <field name="documento"/>
		  <field name="tipo"/>
		  <field name="qty"/>
		  <field name="saldo"/>
		  <field name="cu"/>
		  <field name="ct"/>
		</form>
	      </field>
	      <group string="Totales" colspan="4" col="2">
		<field name="total_in" readonly="1"/>
		<field name="total_in_usd" readonly="1"/>
		<field name="total_out" readonly="1"/>
		<field name="total_out_usd" readonly="1"/>
		<field name="total" readonly="1"/>
	      </group>
	    </page>
	  </notebook>
	  
        </form>
      </field>
    </record>

