		<!--
		==================================
        Configuracion de salidas al campo
        ==================================
        -->
        
        <record id="view_salidas_configuration_form" model="ir.ui.view">
            <field name="name">salidas.configuration.form</field>
            <field name="model">salidas.configuration</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Configuración de Salidas al campo">
                	<group colspan="4">
                    	<field name="name" colspan="4" required="1"/>
                    	<newline/>
