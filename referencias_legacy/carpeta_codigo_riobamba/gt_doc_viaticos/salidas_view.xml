        <record id="view_salidas_solicitud_form_usuario" model="ir.ui.view">
            <field name="name">salidas.solicitud.form</field>
            <field name="model">salidas.solicitud</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Registro de Salidas">
                	<group colspan="4" col="4">
                		<separator colspan="4"/>
                		<field name="state" widget="statusbar" statusbar_visible="aprobado1,aprobado2,aprobado3,done" statusbar_colors='{"draft":"black","aprobado1":"blue","aprobado2":"blue","aprobado3":"blue","done":"green","pagado":"green","descartado":"grey"}'/>
                		<separator colspan="4"/>
                    	<notebook colspan="4">
	                    	<page string="Informe de Salida">
		                    	<group string="Detalle del Funcionario" colspan="4" col="8">
		                    		<field name="name" colspan="2" readonly="1"/>
			                    	<field name="fecha_solicitud" colspan="2" readonly="1"/>
		                    		<field name="department_id" colspan="4" readonly="1" widget="selection"/>
		                    		<newline/>
		                    		<field name="employee_id" colspan="4" required="1" on_change="buscar_departamento(employee_id)" attrs="{'readonly':[('state','!=','draft')]}" domain="[('user_id.id','=',uid)]"/>
		                    		<field name="job_id" colspan="4" readonly="1" widget="selection"/>
		                    	</group>
		                    	<group string="Detalle del Destino" colspan="4" col="4">
		                    		<field name="ciudad_id" colspan="2" required="1" attrs="{'readonly':[('state','!=','draft')]}" domain="[('zona_viaticos_id','!=',False),('zona_viaticos_id.salidas','=',True)]"/>
		                    		<field name="parroquia_id" colspan="2" required="1" attrs="{'readonly':[('state','!=','draft')]}" domain="[('canton_id','=',ciudad_id)]"/>
		                    		<newline/>
		                    		<field name="fecha_salida" colspan="2" required="1" on_change="calcular_tiempo(fecha_salida, fecha_retorno)" attrs="{'readonly':[('state','!=','draft')]}"/>
			                		<field name="fecha_retorno" colspan="2" required="1" on_change="calcular_tiempo(fecha_salida, fecha_retorno)" attrs="{'readonly':[('state','!=','draft')]}"/>
			                		<field name="tiempo" colspan="2" on_change="calcular_tiempo(fecha_salida, fecha_retorno)" readonly="1"/>
			                		<field name="vehiculo" colspan="2" attrs="{'readonly':[('state','!=','draft')]}"/>
		                    	</group>
		                    	<group colspan="4" string="Actividades">
		                    		<field name="actividades" nolabel="1" attrs="{'readonly':[('state','!=','draft')]}" required="1"/>
		                    	</group>
		                    	<group string="Talento Humano" colspan="4" groups="base.group_hr_manager" col="4">
		                    		<field name="valor" colspan="2" readonly="1"/>
		                    		<field name="period_id" colspan="2" attrs="{'readonly':[('state','!=','aprobado3')],'required':[('state','=','aprobado3',)]}" groups="base.group_hr_manager"/>
		                    		<button colspan="2" name="aprobar3_realizado" string="Aprobación TTHH" type="object" states="aprobado3" icon="gtk-apply"  groups="base.group_hr_manager" confirm="CON ESTA ACCION EL REGISTRO ES APROBADO POR TALENTO HUMANO, CONFIRMA QUE DESEA CONTINUAR?"/>
									<button colspan="2" name="descartar" string="Rechazar" type="object" states="aprobado3" icon="gtk-close" groups="base.group_hr_manager" confirm="ESTA ACCION DESCARTA EL PROCESO DE SALIDA AL CAMPO, CONFIRMA QUE DESEA CONTINUAR?"/>
		                    	</group>
		                    	<group colspan="4" col="8">
		                    		<button name="draft_aprobar1" type="object" string="Enviar Informe" colspan="2" states="draft" icon="gtk-apply" confirm="ESTA ACCION SE ENVIA EL INFORME, CONFIRMA QUE DESEA CONTINUAR?"/>
		                    		<button name="descartar" type="object" string="Rechazar" colspan="2" states="draft" icon="gtk-close" confirm="ESTA ACCION DESCARTA EL PROCESO DE SALIDA AL CAMPO, CONFIRMA QUE DESEA CONTINUAR?"/>
		                    	</group>
			                </page>
		                	<page string="Historial">
			                	<field name="log_id" nolabel="1" readonly="1">
			                		<tree string="Historial de acciones en la Salida">
			                			<field name="fecha"/>
			                			<field name="user_id"/>
			                			<field name="name"/>
			                		</tree>
			                		<form string="Historial de acciones en la salida">
			                			<group colspan="4" col="4">
				                			<field name="fecha" colspan="2"/>
				                			<field name="user_id" colspan="2"/>
				                			<field name="name" colspan="4"/>
			                			</group>
			                		</form>
			                	</field>
			                </page>
		                </notebook>
                    </group>
                </form>
            </field>
        </record>
        
        <record id="view_salidas_solicitud_form_secretaria" model="ir.ui.view">
