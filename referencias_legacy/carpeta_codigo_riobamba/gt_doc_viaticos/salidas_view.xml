		                    		<button colspan="2" name="aprobar3_realizado" string="Aprobación TTHH" type="object" states="aprobado3" icon="gtk-apply"  groups="base.group_hr_manager" confirm="CON ESTA ACCION EL REGISTRO ES APROBADO POR TALENTO HUMANO, CONFIRMA QUE DESEA CONTINUAR?"/>
									<button colspan="2" name="descartar" string="Rechazar" type="object" states="aprobado3" icon="gtk-close" groups="base.group_hr_manager" confirm="ESTA ACCION DESCARTA EL PROCESO DE SALIDA AL CAMPO, CONFIRMA QUE DESEA CONTINUAR?"/>
		                    	</group>
		                    	<group colspan="4" col="8">
		                    		<button name="draft_aprobar1" type="object" string="Enviar Informe" colspan="2" states="draft" icon="gtk-apply" confirm="ESTA ACCION SE ENVIA EL INFORME, CONFIRMA QUE DESEA CONTINUAR?"/>
		                    		<button name="descartar" type="object" string="Rechazar" colspan="2" states="draft" icon="gtk-close" confirm="ESTA ACCION DESCARTA EL PROCESO DE SALIDA AL CAMPO, CONFIRMA QUE DESEA CONTINUAR?"/>
		                    	</group>
			                </page>
		                	<page string="Historial">
			                	<field name="log_id" nolabel="1" readonly="1">
			                		<tree string="Historial de acciones en la Salida">
			                			<field name="fecha"/>
			                			<field name="user_id"/>
			                			<field name="name"/>
			                		</tree>
			                		<form string="Historial de acciones en la salida">
			                			<group colspan="4" col="4">
				                			<field name="fecha" colspan="2"/>
				                			<field name="user_id" colspan="2"/>
				                			<field name="name" colspan="4"/>
			                			</group>
			                		</form>
			                	</field>
			                </page>
		                </notebook>
                    </group>
                </form>
            </field>
        </record>
        
        <record id="view_salidas_solicitud_tree" model="ir.ui.view">
            <field name="name">salidas.solicitud.tree</field>
            <field name="model">salidas.solicitud</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Solicitud para Salidas al Campo" colors="black:state == 'draft';blue:state in ('aprobado1','aprobado2','aprobado3');green:state in ('done','pagado');grey:state == 'descartado'">
                	<field name="name"/>
                	<field name="employee_id"/>
                	<field name="department_id"/>
                	<field name="ciudad_id"/>
                	<field name="parroquia_id"/>
                	<field name="fecha_salida"/>
                	<field name="fecha_retorno"/>
                	<field name="vehiculo"/>
                	<field name="valor"/>
                	<field name="period_id"/>
                	<field name="state"/>
                	<separator orientation="vertical"/>
                	<!--button string="Enviar Informe" name="draft_aprobar1" states="draft" type="object" icon="gtk-apply" confirm="CON ESTA ACCION SE ENVIA EL INFORME DE SALIDA, CONFIRMA QUE DESEA CONTINUAR?"/-->
					<button string="Aprobación Jefe Superior" name="aprobar1_aprobar2" states="aprobado1" type="object" icon="gtk-apply" groups="gt_doc_viaticos.group_via_jefe" confirm="CON ESTA ACCION EL JEFE SUPERIOR APRUEBA EL REGISTRO DE SALIDA, CONFIRMA QUE DESEA CONTINUAR?"/>
					<button string="Aprobación Coordinador" name="aprobar2_aprobar3" states="aprobado2" type="object" icon="gtk-apply"  groups="gt_doc_viaticos.group_via_coordinador" confirm="CON ESTA ACCION EL COORDINADOR APRUEBA EL REGISTRO DE SALIDA, CONFIRMA QUE DESEA CONTINUAR?"/>
					<!--button string="Aprobación TTHH" name="aprobar3_realizado" states="aprobado3" type="object" icon="gtk-apply"  groups="base.group_hr_manager" confirm="CON ESTA ACCION EL REGISTRO ES APROBADO POR TALENTO HUMANO, CONFIRMA QUE DESEA CONTINUAR?"/-->
					<button name="descartar" type="object" string="Rechazar" states="aprobado1" icon="gtk-close" confirm="ESTA ACCION DESCARTA EL PROCESO DE SALIDA AL CAMPO, CONFIRMA QUE DESEA CONTINUAR?" groups="gt_doc_viaticos.group_via_jefe"/>
					<button name="descartar" type="object" string="Rechazar" states="aprobado2" icon="gtk-close" confirm="ESTA ACCION DESCARTA EL PROCESO DE SALIDA AL CAMPO, CONFIRMA QUE DESEA CONTINUAR?" groups="gt_doc_viaticos.group_via_coordinador"/>
                </tree>
            </field>
        </record>
        
        <record id="view_salidas_solicitud_search" model="ir.ui.view">
            <field name="name">salidas.solicitud.search</field>
            <field name="model">salidas.solicitud</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Solicitud para Salidas al Campo">
                	<group col='4' colspan='4'>
                		<filter icon="gtk-edit" name="state_draft" string="Borrador" domain="[('state','=','draft')]"/>
                		<filter icon="gtk-file" name="state_aprobado1" string="Informes Ingresados" domain="[('state','=','aprobado1')]"/>
                		<filter icon="gtk-file" name="state_aprobado2" string="Aprobado Jefe Superior" domain="[('state','=','aprobado2')]"/>
                		<filter icon="gtk-file" name="state_aprobado3" string="Aprobado Coordinador General" domain="[('state','=','aprobado3')]"/>
                		<filter icon="gtk-file" name="state_done" string="Aprobado TTHH" domain="[('state','=','done')]"/>
                		<filter icon="gtk-close" name="state_descartado" string="Rechazados" domain="[('state','=','descartado')]"/>
                		<newline/>
                		<!--separator orientation="vertical"/-->
                		<field name="name" string="Numero de orden"/>
                		<field name="employee_id" string="Empleado"/>
                		<field name="department_id" string="Departamento"/>
                		<field name="ciudad_id" string="Cantón"/>
                		<field name="parroquia_id" string="Parroquia"/>
                		<field name="vehiculo" string="Vehículo"/>
                		<field name="state" string="Estado"/>
                		<newline/>
                		<field name="fecha_salida" string="Salida desde" filter_domain="[('fecha_salida','&gt;=',self)]"/>
                        <field name="fecha_retorno" string="Llegada hasta" filter_domain="[('fecha_retorno','&lt;=',self)]"/>
                        <separator orientation="vertical"/>
                        <field name="period_id" string="Mes de pago"/>
                	</group>
                	<newline/>
					<group expand="1" string="Agrupar por..." colspan="4" col="10">
						<filter name="group_employee_id" string="Empleado" icon="terp-personal" context="{'group_by':'employee_id'}"/>
						<filter name="group_department_id" string="Departamento" icon="terp-partner" context="{'group_by':'department_id'}"/>
						<filter name="group_ciudad_id" string="Destino" icon="gtk-home" context="{'group_by':'ciudad_id'}"/>
						<filter name="group_vehiculo" string="Vehiculo" icon="gtk-home" context="{'group_by':'vehiculo'}"/>
						<filter name="group_period_id" string="Mes de pago" icon="gtk-file" context="{'group_by':'period_id'}"/>
						<filter name="group_state" string="Estado" icon="terp-stock_symbol-selection" context="{'group_by':'state'}"/>
                	</group>
                </search>
            </field>
        </record>
        
        <record id="view_salidas_solicitud_calendar" model="ir.ui.view">
            <field name="name">Registo de Salidas</field>
            <field name="model">salidas.solicitud</field>
            <field name="type">calendar</field>
            <field name="arch" type="xml">
                <calendar string="Salidas al campo" color="employee_id" date_start="fecha_salida" date_stop="fecha_retorno">
                    <field name="employee_id"/>
                    <field name="fecha_salida"/>
                    <field name="fecha_retorno"/>
                </calendar>
            </field>
        </record>
        
