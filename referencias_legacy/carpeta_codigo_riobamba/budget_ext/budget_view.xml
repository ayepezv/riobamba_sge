    <record id="view_budget_concejo" model="ir.ui.view">
      <field name="name">Presupuesto Aprobado Concejo</field>
      <field name="model">budget.concejo</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Detale">
	  <group colspan="4">
	    <field name="state" colspan="2" widget="statusbar"/>
	  </group>
	  <group colspan="4">
	    <button icon="gtk-execute" name="clonaConcejo" string="Clonar Presupuesto Concejo" type="object" states='Borrador'/>	    
	    <button icon="gtk-ok" name="apruebaConcejo" string="Aprobar Presupuesto Concejo" type="object" states='Borrador'/>	    
	  </group>
	  <group colspan="4">
	    <newline/>
	    <field name="name" widget="selection" colspan="2" required="1" attrs="{'readonly': [('state', '!=','Borrador')]}"/>
	    <field name="year_id" colspan="2" required="1" attrs="{'readonly': [('state', '!=','Borrador')]}"/>
	  </group>
	  <newline/>
	  <notebook colspan="4">
	    <page string="Detalle Partidas Ingreso">
	      <field name="line_ids2" nolabel="1" attrs="{'readonly': [('state', '!=','Borrador')]}">
		<tree>
		<field name="program_id"/>
		<field name="name"/>
		<field name="budget_id"/>
		<field name="inicial" sum="TOTAL"/>
		</tree>
	      </field>
	    </page>
	    <page string="Detalle Partidas Gasto">
	      <field name="line_ids" nolabel="1" attrs="{'readonly': [('state', '!=','Borrador')]}">
		<tree>
		<field name="program_id"/>
		<field name="name"/>
		<field name="inicial" sum="TOTAL"/>
		</tree>
	      </field>
	    </page>
	  </notebook>
        </form>
      </field>
      </record>

