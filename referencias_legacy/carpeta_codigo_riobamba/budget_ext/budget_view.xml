    <record id="view_partida_financia_head_form" model="ir.ui.view">
      <field name="name">view.partida.financia.head.form</field>
      <field name="model">partida.financia.head</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
	<form string="Partida cuenta financiera">
	  <group colspan="4" col='4'>
	    <field name="name" required="1" colspan="2"/>
	    <button icon="gtk-execute" name="actualizaFinancia" string="Actualizar" type="object" colspan="1" confirm="Si usted ejecuta esta accion se elimina lo que estuvo ya cargado y se tren nuevamente las partidas del presupuesto de ingresos"/>
	    <!--button icon="gtk-execute" name="clonaFinanciaHead" string="Clonar Anio Anterior" type="object" colspan="1"/-->
	  </group>
	  <notebook colspan="4">
	    <page string="Detalle">
	      <field name='line_ids' nolabel='1'>
		<tree>
		  <!--field name='project_id' required="1" domain="[('poa_id','=',parent.name.id)]"/-->
		  <field name='item_id' required="1"/>
		  <field name='financiera_id' required="1"/>
		  <!--field name='budget_id' readonly="1"/-->
		  <field name='name' readonly="1" invisible="1"/>
		  <field name='is_reform'/>
		  <field name='is_recauda'/>
		  <field name='monto' required="1"/>
		  <field name='reforma'/>
		  <field name='codificado'/>
		  <!--field name='recaudado' required="1"/>
		  <field name='porcentaje' required="1"/-->
		</tree>
		<form>
		  <field name='item_id' required="1"/>
