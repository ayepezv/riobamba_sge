<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>

    <record id="view_rpt_cert" model="ir.ui.view">
      <field name="name">Cert Line</field>
      <field name="model">rpt.cert</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Detale">
	  <group colspan="4" col="5">
	    <field name="certificate_id" colspan="2" domain="[('state','=','commited')]"/>
	    <button icon="gtk-execute" name="computeCertLine" string="Generar" type="object"/>	    
	    <button icon="gtk-execute" name="exporta_excel_cert" string="Exporta Excel" type="object" colspan="1"/>
	  </group>
	  <group colspan="4">
	    <field name='fecha_desde' required="1"/>
	    <field name='fecha_hasta' required="1"/>
   	  </group>
	  <group>
	    <field name="datas_fname" colspan="4" invisible="1"/>
	    <field name="datas" filename="datas_fname" colspan="4" readonly="1"/>
	  </group>
	  <group colspan="4">
   	  </group>
	  <notebook>
	    <page>
	      <field name="line_ids" nolabel="1">
		<tree>
		  <field name="certificate_id"/>
		  <field name="partner_id"/>
		  <field name="date"/>
		  <field name="comprometido" sum="TOTAL"/>
		  <field name="pagado" sum="TOTAL"/>
		</tree>
		<form>
		  <field name="certificate_id"/>
		  <field name="line_ids" colspan="4"  nolabel="1">
		    <tree>
		      <field name="program_id"/>
		    </tree>
		    <form>
		      <field name="program_id"/>
		      <field name="line_ids" colspan="4" nolabel="1">
			<tree>
			  <field name="budget_id"/>
			  <field name="financiera"/>
			  <field name="comprometido" sum="TOTAL"/>
			  <field name="pagado" sum="TOTAL"/>
			</tree>
		      </field>
		    </form>
		  </field>
		</form>
	      </field>
	    </page>
	  </notebook>
	  <newline/>
        </form>
      </field>
    </record>
    
    <record id="action_rpt_cert" model="ir.actions.act_window">
      <field name="name">Certificada detalle</field>
      <field name="res_model">rpt.cert</field>
      <field name="type">ir.actions.act_window</field>
      <field name="view_type">form</field>
      <field name="target">new</field>
      <field name="view_mode">form</field>
    </record>
      
      <menuitem id="menu_rpt_cert"
		icon="STOCK_EXECUTE"
		action="action_rpt_cert"
		parent="gt_budget.menu_budget_queries_base"
		name="Rpt Cert Line"
		sequence="16"/>

