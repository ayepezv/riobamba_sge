        <record id="hr_holidays.edit_holiday_new" model="ir.ui.view">
          <field name="name">Permisos y Vacaciones</field>
          <field name="model">hr.holidays</field>
          <field name="type">form</field>
          <!--field name="inherit_id" ref="hr_holidays.edit_holiday_new"/-->
          <field name="arch" type="xml">
            <!--xpath expr="/form" position="replace"-->
	    <form string="Permisos y Vacaciones">
	      <field name="holiday_type" on_change="onchange_type(holiday_type)" attrs="{'readonly':[('state','!=','draft')]}" 
		     string="Mode" groups="base.group_hr_manager" invisible="1"/>
	      <field name="name" colspan="2" string="Descripción" attrs="{'readonly':[('state','!=','draft')]}" invisible="1"/>
	      <field name="code" readonly="1" invisible="1"/>
	      <field name="employee_id" colspan="2" attrs="{'required':[('holiday_type','=','employee')]}"/>
	      <field name="department_id" colspan="2" readonly="1"/>
	      <notebook colspan="4">
	        <page string="General">
	          <field name="holiday_status_id" colspan="2" context="{'employee_id':employee_id}" domain="[('code','=','FALTA')]"/>
	          <!--field name="holiday_status_id" colspan="2" on_change="onchange_sec_id(holiday_status_id)" context="{'employee_id':employee_id}"/-->
	          <field name="tipo_vacaciones" colspan="2" readonly="1" invisible="1"/>
	          <!--field name="date_start" colspan="2"/-->
	          <field name="date_from" colspan="2" on_change="onchange_date_from(date_to, date_from)" required="1"/>
	          <field name="date_to" colspan="2" on_change="onchange_date_from(date_to, date_from)" required="1"/>
	          <field name="total_dias" colspan="2"/>
	          <field name="horas" colspan="2"/>
		  <field name="line_id" colspan="2" readonly="1"/>
	          <field name="number_of_days_temp" colspan="2" string="Numero de dias" invisible="1"/>
	          <field name="double_validation" colspan="2" invisible="1"/>
	          <newline/>
	          <!--field name="manager_id"/-->
	          <field name="manager_id2" attrs="{'invisible':[('double_validation', '!=', True)]}"/>
	          <separator string="Razones" colspan="4"/>
	          <field name="notes" nolabel="1" colspan="4"/>
	          <newline/>
	          <group colspan="4" col="8">
		    <field name="state" widget="statusbar" statusbar_visible="draft,confirm,validate" 
			   statusbar_colors='{"confirm":"blue","validate1":"blue","refuse":"red"}' colspan="2"/>
	            <button string="Rechazar" name="refuse" states="confirm,validate1" type="workflow" icon="gtk-no" groups="base.group_hr_user,base.group_hr_manager"/>
	            <button string="Confirmar" name="confirm" states="draft" type="workflow" icon="gtk-yes"/>
	            <button string="Primera Aprobación" name="validate" states="confirm" type="workflow" icon="gtk-apply" groups="base.group_hr_user"/>
	            <button string="Segunda Aprobación" name="second_validate" states="validate1" type="workflow" icon="gtk-apply" groups="base.group_hr_user"/>
		    <button name="recalcula_descuento_falta" states="validate1,validate" string="Recalcular" 
			    type="object" icon="gtk-execute"/> 
		    <button name="print_permiso" colspan="4" states="validate1" string="Imprimir" 
			    type="object" icon="gtk-print"/> 
	            <!--button string="Set to Draft" name="set_to_draft" states="refuse,validate" type="object" icon="gtk-convert" groups="base.group_hr_user"/-->
	          </group>
	        </page>
