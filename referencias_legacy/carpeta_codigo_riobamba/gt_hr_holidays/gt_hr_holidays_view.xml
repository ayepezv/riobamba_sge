    <record id="view_saldo_vac" model="ir.ui.view">
      <field name="name">Saldo Vacaciones</field>
      <field name="model">saldo.vacempleado</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Reporte saldo vacaciones">
	  <group>
	    <field name="datas_fname" colspan="3" invisible="1"/>
