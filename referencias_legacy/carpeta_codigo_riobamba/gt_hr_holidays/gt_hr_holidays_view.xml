	    <button icon="gtk-execute" name="exporta_saldovac" string="Exporta Excel" type="object" colspan="1"/>
	  </group>
	  <group col="4" colspan="4">
	    <field name='line_ids' readonly="1" colspan="4" nolabel="1">
	      <tree>
		<field name='employee_id'/>
		<field name='dias'/>
	      </tree>
	    </field>
	    <newline/>
	    <group col="3" colspan="1">
	      <button icon="gtk-cancel" special="cancel"
	              string="Salir" />
	      <button name="calcula_saldovac" type="object" icon="gtk-execute"  string="Ejecutar"/>
	    </group>
	  </group>
        </form>
      </field>
    </record>
	  
