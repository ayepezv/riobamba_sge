		<field name="model">gt.report.type</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Tipo de Sanción" toolbar="1">
				<field name="name" colspan="1" string="Nombre de Sanción"/> 
			</tree>
		</field>
	</record>
	<record id="view_search_gt_report_type" model="ir.ui.view">
		<field name="name">Tipos de Notificaciones</field>
        <field name="model">gt.report.type</field>
        <field name="type">search</field>
        <field name="arch" type="xml">
        	<search string="Tipos de Sanción">
            	<group>
                	<field name="name" />											
				</group>              			  		
			</search>
		</field>
	</record>	
	<record id="action_gt_report_type" model="ir.actions.act_window">
    	<field name="name">Tipos de Notificaciones</field>             
        <field name="res_model">gt.report.type</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
	</record>    
	<!--REPORTE de CREACION DE HISTORIALES-->	
	<record  id="view_form_create_hisroty" model="ir.ui.view">
		<field name="name">Creación de Registros</field>
		<field name="model">gt.wizard.history</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
		<form string="Creación de Registros">
			<group  colspan="4" col="6">
				<field name="name" colspan="3" readonly="1"/>
				<field name="employee_id" colspan="3" readonly="1"/>
				<field name="regime" colspan="2" attrs="{'readonly':[('state','in',['close','open','cancel'])]}" required="1"/>
				<field name="first_history" colspan="2" attrs="{'readonly':[('state','in',['close','open','cancel'])]}"/>
				<field name="indicted" colspan="2"/>				
				

			</group>
			<group colspan="4" col="6" string= "Registro Previo" 
				attrs="{'invisible': [('first_history', '=',True)], 'required': [('first_history', '=',False)]}">
				<!--field name="prev_history" colspan="2" on_change="load_date(prev_history)" domain="[('indicted','=',False)]" attrs="{'required': [('first_history', '=',False)], 'readonly':[('state','in',['close','open','cancel'])] }"/-->
				<field name="prev_history" colspan="2" on_change="load_date(prev_history)" domain="[('indicted','=',False),('regime','=',regime)]" attrs="{'required': [('first_history', '=',False)], 'readonly':[('state','in',['close','open','cancel'])] }"/>
				<field name="prev_start_date" colspan="2" 
					on_change="load_date_start(prev_end_date)"/>
				<field name="prev_end_date" colspan="2" attrs="{'required': [('first_history', '=',False)], 'readonly':[('state','in',['close','open','cancel'])]}"/>
			</group>
			<group colspan="4" col="6" string= "Nuevo Registro">
				<field name="name_history" colspan="2" attrs="{'readonly':[('state','in',['close','open','cancel'])]}"/>
				<field name="start_date" colspan="2" attrs="{'readonly':[('state','in',['close','open','cancel'])]}" />
				<field name="end_date" colspan="2" attrs="{'readonly':[('state','in',['close','open','cancel'])]}"/>
			</group>	
			<group colspan="2" col="4">	
				<button name="create_history" states="draft" colspan="2" type="object" icon="gtk-execute" string="Confirmar Información"
					attrs="{'readonly':[('state','in',['close','open'])]}"/>		
				<button icon="gtk-cancel" states="draft" type="object" name="abrir_manual" string="Abrir Manual" colspan="2"
					confirm='Esta solicitand abrir el registro de historiales manualmente.    Desea continuar'/>
				<button name="unique_his" states="open" colspan="1" type="object" icon="gtk-execute" string="Agregar Registro individual"
					attrs="{'readonly':[('state','in',['draft','close'])]}"/>																																														
			</group>
			<group colspan="2" col="4">
				<button icon="gtk-cancel" states="draft" type="object" name="cancelar_historial" string="Cancelar" colspan="1"
					confirm='Esta acción no se podrá revertir.     Desea continuar'/>	
				<button icon="gtk-cancel" states="open" type="object" name="cerrar_historial" string="Cerrar" colspan="1"
					confirm='Esta solicitand cerrar el registro de historiales manualmente.    Desea continuar'/>
			</group>	
			<newline/> 
			<group colspan="4" col="4" string= "Registros Asignados">
				<field name="history_ids" colspan="4" nolabel="1"/>	
				<field name="state" colspan="4" readonly="1"/>						
			</group>				
		</form>
		</field>
	</record> 
	<record id="view_tree_create_history" model="ir.ui.view" >
		<field name="name">Creación de Registros</field>
		<field name="model">gt.wizard.history</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Creación de Registros" toolbar="1">
				<field name="name" colspan="1" string="Nombre del Registro" /> 
				<field name="employee_id" colspan="1" string="Nombre del Empleado"/>
				<field name="name_history" colspan="4" readonly="1" string="Nombre del Registro"/>
				<field name="regime"/>
				<field name="state"/>
			</tree>
		</field>
	</record>
	<record id="view_search_create_history" model="ir.ui.view">
		<field name="name">Creación de Registros</field>
        <field name="model">gt.wizard.history</field>
        <field name="type">search</field>
        <field name="arch" type="xml">
        	<search string="Creación de Registros">
            	<group>
                	<field name="name"  />
                	<field name="employee_id" string="Nombre del Empleado"/>
                	<field name="name_history" colspan="4" readonly="1" string="Nombre del Registro"/>
                	<field name="regime"/>
				</group> 
				<newline/>     
				<group expand="1"  string="Group By...">	                       
		        	<filter string="Nombre" icon="terp-go-home" domain="[]" context="{'group_by' : 'name_history'}" />
		            <separator orientation="vertical"/>	                                              
		            <filter string="Régimen" icon="terp-go-month" domain="[]" context="{'group_by' : 'regime'}" />
		            <separator orientation="vertical"/>	 		            
				</group>        			  		
			</search>
		</field>
	</record>	
	<record id="action_create_history" model="ir.actions.act_window">
    	<field name="name">Creación de Registros</field>             
        <field name="res_model">gt.wizard.history</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
	</record>  
	<!--CONFIGURACION-->	
	<record  id="view_form_field_workable" model="ir.ui.view">
		<field name="name">Configuracion</field>
		<field name="model">gt.field.workable</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
		<form string="Configuraciones">
			<group colspan="4" col="4" >
