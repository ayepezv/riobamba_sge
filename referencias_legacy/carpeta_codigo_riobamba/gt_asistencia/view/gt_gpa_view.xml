<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data> 		

	<!--MENU TIPOS DE SANCION-->
	<record id="view_form_gt_clock_name" model="ir.ui.view">
		<field name="name">Reloj</field>
	    <field name="model">gt.clock.name</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Reloj">
				<field name="name" colspan="2"/>
				<field name="ip" colspan="2"/>					  				
			</form>
		</field>
	</record>	
		<record id="view_tree_gt_clock_name" model="ir.ui.view" >
		<field name="name">Reloj</field>
		<field name="model">gt.clock.name</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Reloj" toolbar="1">
				<field name="name" /> 
				<field name="ip"/>				 
			</tree>
		</field>
	</record>
	<record id="action_gt_clock_name" model="ir.actions.act_window">
    	<field name="name">Reloj</field>             
        <field name="res_model">gt.clock.name</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
	</record>	
	
	
	<!--MENU TIPOS DE SANCION-->
	<record id="view_form_gt_assent_type" model="ir.ui.view">
		<field name="name">Tipos de Sanción</field>
	    <field name="model">gt.assent.type</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Tipo de Sanción">
				<group colspan="4" col="4" >
					<field name="name" colspan="2"/>        		
					<field name="code" colspan="2"/>					  
					<!--field name="range" colspan="2"/--> 				
					<field name="min_assent" colspan="2" attrs="{'invisible': [('max_assent', '=',True)]}"/>
					<field name="max_assent" colspan="2" attrs="{'invisible': [('min_assent', '=',True)]}"/>				 						  								
					<field name="notification" colspan="1" attrs="{'invisible': [('min_assent', '=',True)]}"/>
					<field name="notification_type" colspan="1" attrs="{'invisible': [('notification', '=',False),('min_assent', '=',True)],
						'required': [('notification', '=',True)]}"/>
				</group >
				<newline/>
				<separator orientation="horizontal"/>
				<group colspan="4" col="4"  string = "Por acumulación de sanciones">
					<field name="next_assent_id" colspan="2"/> 
					<field name="number_max" colspan="2"/>
				</group >
				<newline/>
				<separator orientation="horizontal"/>
				<group colspan="4" col="4"  string = "Por acumulación de tiempo">
					<field name="max_month" colspan="1"/>  								
					<field name="minutes_max_month" colspan="1" attrs="{'invisible': [('max_month', '=',False)],
						'required': [('max_month', '=',True)]}"/>
					<field name="max_anual" colspan="1"/>  								
					<field name="minutes_max_anual" colspan="1" attrs="{'invisible': [('max_anual', '=',False)],
						'required': [('max_anual', '=',True)]}"/>
				</group>  

			</form>
		</field>
	</record>
	<record id="view_tree_gt_assent_type" model="ir.ui.view" >
		<field name="name">Tipos de Sanción</field>
		<field name="model">gt.assent.type</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Tipo de Sanción" toolbar="1">
				<field name="name" colspan="1" string = 'Nombre de Sancion'/> 
				<field name="code" colspan="1"/>
				<field name="number_max" colspan="2"/>
				<field name="next_assent_id" colspan="2"/>  
			</tree>
		</field>
	</record>
	<record id="view_search_gt_assent_type" model="ir.ui.view">
		<field name="name">Tipos de Sanción</field>
        <field name="model">gt.assent.type</field>
        <field name="type">search</field>
        <field name="arch" type="xml">
        	<search string="Tipos de Sanción">
            	<group>
                	<field name="name" />
                	<field name="code"/>											
				</group>              			  		
			</search>
		</field>
	</record>	
	<record id="action_gt_assent_type" model="ir.actions.act_window">
    	<field name="name">Tipos de Sanción</field>             
        <field name="res_model">gt.assent.type</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
	</record>        

	<!--MENU HISTORIAL-->	
	<record id="view_form_gt_history" model="ir.ui.view">
		<field name="name">Registro</field>
	    <field name="model">gt.history</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Registro">
				<group colspan="2" col="8">
					<field name="name" colspan="3" readonly="1"/>   
					<field name="employee_id" colspan="3" readonly="1"/>
					<field name="start_date" colspan="2" readonly="1"/>
					<field name="end_date" colspan="3" readonly="1"/>     		
					<field name="anual_minute" colspan="3" />
					<field name="month_minute" colspan="2"/>
				</group>
				<newline/>
				<group colspan="2" col="8" string="Conteo de sanciones">
					<field name="counter_assent" colspan="2"/>
					<field name="type_assent_id" colspan="2"/>
					<field name="counter_assent_acu" colspan="2"/>
					<field name="type_assent_id_acu" colspan="2"/>
				</group>
				<newline/>
				<field name="event_ids" colspan="4" nolabel="1">
					<tree string="Asistencia" editable="top">				
						<field name="action" colspan="1" select="1"/>		
						<field name="action_type" colspan="1" select="1"/>													
						<field name="assent_type_id" colspan="1" select="1"/>
						<field name="name" colspan="1" select="1"/>
						<field name="date_spetial" colspan="1" select="1"/>
						<field name="not_normal" colspan="1" select="1"/>
						<field name="indicted" colspan="1" />
						<field name="state" colspan="1" select="1" readonly="1"/>
						
					</tree>
							
				</field>
				<field name="state" colspan="4"/>	
				<newline/>
				<!--group colspan="4" col="6">
                	<field name="state"/>
                	<button name="gt_gpa_history_open" string="Abrir" states="draft" icon="gtk-go-forward"/>
                	<button name="gt_gpa_history_close" string="Cerrar" states="open" icon="gtk-jump-to"
                		confirm="Cierra el historial"/>                
            	</group--> 			
			</form>
		</field>
	</record>
	<record id="view_tree_gt_history" model="ir.ui.view" >
		<field name="name">Registro</field>
		<field name="model">gt.history</field>
		<field name="type">tree</field>
		<field name="arch" type="xml">
			<tree string="Registro" toolbar="1">
				<field name="name" colspan="1" string="Nombre de Registro"/>        		
				<field name="employee_id" string = "Apellido de empleado"/>
				<field name="start_date" colspan="1" string ="Fecha en que inicia"/>
				<field name="end_date" colspan="1" string = "Fecha en que finaliza"/>	
				<field name="state"/>
			</tree>
		</field>
	</record>
	<record id="view_search_gt_history" model="ir.ui.view">
    	<field name="name">Registro</field>
        <field name="model">gt.history</field>
        <field name="type">search</field>
        <field name="arch" type="xml">
        	<search string="Search Curso">
				<group>					
					<field name="name" string="Nombre de Registro"/>	
					<field name="employee_id" string = "Empleado"/>
					<field name="start_date"  string ="Fecha en que inicia"/>
					<field name="end_date" string = "Fecha en que finaliza"/>									
				</group>
				<newline/>
	            <group expand="1"  string="Group By...">	                       
		        	<filter string="Empleado" icon="terp-go-home" domain="[]" context="{'group_by' : 'employee_id'}" />
		            <separator orientation="vertical"/>	                                              
		            <filter string="Fecha inicio" icon="terp-go-month" domain="[]" context="{'group_by' : 'start_date'}" />
		            <separator orientation="vertical"/>	 
		            <filter string="Fecha fin" icon="terp-gtk-jump-to-ltr" domain="[]" context="{'group_by' : 'end_date'}" />
		            <separator orientation="vertical"/>
		            <filter string="Estado" icon="terp-gtk-jump-to-ltr" domain="[]" context="{'group_by' : 'state'}" />
		            <separator orientation="vertical"/>
				</group>
			</search>
		</field>
	</record>
	<record id="action_gt_history" model="ir.actions.act_window">
    	<field name="name">Registro</field>             
        <field name="res_model">gt.history</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
	</record>
	<!-- MENU EVENTOS -->     
	<record id="view_form_gt_attendance" model="ir.ui.view">
      	<field name="name">Evento</field>
      	<field name="model">hr.attendance</field>
		<field name="type">form</field>
		<field name="priority">10</field>
		<field name="arch" type="xml">
		<form string="Evento"> <!--attrs="{'readonly':[('state','in',['close','open'])]}"-->
			<group colspan="4" col="4" >
				<field name="name" colspan="4" string='Fecha de Procesamiento' readonly = "1"/>        		
				<field name="action" colspan="2" select="1"/>				
				<field name="action_type" colspan="2"/>								
				<!--field name="state" colspan="2"  attrs="{'readonly':[('state','=','open')]}" /-->
				<!--field name="out" colspan="2" readonly = "1" /-->
				<field name="date_dbo" colspan="2"/>
				<field name="date_spetial" colspan="2"/><!-- attrs="{ 'invisible':[('date_dbo','!=','')],  'required':[('date_dbo','=','')]}"/-->	
				<!--field name="date_activate" colspan="4" readonly = "1"/-->
				<field name="reason" colspan="4"  attrs="{ 'required':[('state','=','cancel')]}"  />
				<field name="who_cancel" colspan="2"  attrs="{ 'invisible':[('state','!=','cancel')],  'required':[('state','=','cancel')]}"  />
				<field name="history_cancel" colspan="2"  attrs="{ 'invisible':[('state','!=','cancel')],  'required':[('state','=','cancel')]}"  />
			</group>
				<!--button name="cancel_history" colspan="1" type="object" icon="gtk-execute" string="Cancelar en historial"  states="cancel"/-->
			<newline/>
			<group colspan="4" col="6" string= "Detalle">
				<field name="gt_employee_id" colspan="3" string='Usuario' readonly="1" attrs="{'required': [('date_dbo', '!=','')]}"/>        		
				<field name="employee_id" colspan="3" string='Empleado'/>
				<field name="day" colspan="1"/>
				<field name="hour_reference" colspan="1" readonly="1"/>				
				<field name="address" colspan="1"/>
				<field name="not_normal" colspan="2"/>
				<field name="indicted" colspan="2" readonly="1" attrs="{ 'invisible':[('not_normal','!=',True)]}"/>
				<field name="mins_delay" colspan="1" readonly="1"
					attrs="{'invisible': [('action_type', 'in',['marc','assent','notification'])], 'required': [('action_type', 'in',['delay','before'])]}"/>												
				<field name="assent_type_id" colspan="3" readonly="1" attrs="{'invisible':[('action','!=','action')]}"/>
				<field name="attendance_id" colspan="3" domain="[('employee_id','=',employee_id)]"  attrs="{'readonly':[('action_type','!=','excuse')]}"/>	
				<field name="body" colspan="6" readonly="1"  
					attrs="{'required': [('action_type', '=','notification')], 'invisible':[('action_type','=','marc')]}"/>
				<newline/>
				<field name="notes" colspan="6"/>
				<field name="state" colspan="6"/>					
			</group>			
		</form>
		</field>
	</record>    

	<record id="view_search_gt_attendance" model="ir.ui.view">
		<field name="name">hr.attendance</field>
        <field name="model">hr.attendance</field>
        <field name="type">search</field>
	<field name="priority">10</field>
        <field name="arch" type="xml">
        	<search string="Marcaciones">
            	<group>                		
					<field name="action" colspan="1" />				
					<field name="action_type" colspan="1"/>              	
					<field name="gt_employee_id" colspan="1" string='Usuario' readonly="1"/>        		
					<field name="employee_id" colspan="1" string='Empleado'/>			
					<!--field name="date_spetial" colspan="1" domain="[('date_spetial','>',date_spetial)]"/>
					<field name="date_spetial" colspan="1"/-->
					<field name="date_spetial" string="Fecha Desde" filter_domain="[('date_spetial','&gt;=',self)]"/>
					<field name="date_spetial" string="Fecha hasta" filter_domain="[('date_spetial','&lt;=',self)]"/>
					<field name="not_normal" colspan="1"/>
				</group>
				<newline/>
	            <group expand="1"  string="Group By...">		            	                       
		        	<filter string="Accion" icon="terp-go-home" domain="[]" context="{'group_by' : 'action'}" />
		            <separator orientation="vertical"/>	                                              
		            <filter string="Razon de Accion" icon="terp-go-month" domain="[]" context="{'group_by' : 'action_type'}" />
		            <separator orientation="vertical"/>	
		            <filter string="Usuario" icon="terp-gtk-jump-to-ltr" domain="[]" context="{'group_by' : 'gt_employee_id'}" />
		            <separator orientation="vertical"/> 
		            <filter string="Empleado" icon="terp-gtk-jump-to-ltr" domain="[]" context="{'group_by' : 'employee_id'}" />
		            <separator orientation="vertical"/>
		            <filter string="Fecha Accion" icon="terp-gtk-jump-to-ltr" domain="[]" context="{'group_by' : 'date_spetial'}" />
		            <separator orientation="vertical"/>  
		            <filter string="Estado" icon="terp-gtk-jump-to-ltr" domain="[]" context="{'group_by' : 'state'}" />
		            <separator orientation="vertical"/>
		             
				</group>				           			  		
			</search>
		</field>
	</record>
	<record id="view_tree_gt_attendance" model="ir.ui.view" >
		<field name="name">Evento</field>
		<field name="model">hr.attendance</field>
		<!--field name="inherit_id" ref="hr_attendance.view_attendance_tree"/-->
		<field name="type">tree</field>
		<field name="priority">10</field>
		
		<field name="arch" type="xml">
			<!--xpath expr="/tree/field[@name='name']" position="replace"-->
			<tree string="Evento" toolbar="1">    				 	
                    <field name="action"/>
                    <field name="action_type"/>            	              	
					<field name="gt_employee_id" colspan="1" string='Usuario' readonly="1"/>        		
					<field name="employee_id" colspan="1" string='Empleado'/>			
					<field name="date_spetial" colspan="1"/>
					<field name="not_normal" colspan="1"/>
					<field name="indicted" colspan="1"/>
					<field name="state" colspan="1"/>
			</tree>
			<!--/xpath-->
		</field>
	</record>         	
	
	<record id="action_gt_attendance" model="ir.actions.act_window">
		<field name="name">Evento</field>             
        <field name="res_model">hr.attendance</field>
        <field name="view_type">form</field>
        <field name="view_mode">form,tree</field>
        <field name="view_id" ref="view_tree_gt_attendance"/>
		<!--field name="view_id" ref="view_form_gt_attendance"/-->
	</record>  
	<!--MENU TIPOS DE REPORTE-->
	<record id="view_form_gt_report_type" model="ir.ui.view">
