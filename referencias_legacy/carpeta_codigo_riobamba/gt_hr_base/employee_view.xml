<!-- ACCIONES DE PERSONAL -->


<record model="ir.ui.view" id="view_accion_personal_form">
  <field name="name">hr.accion.personal.form</field>
  <field name="model">hr.accion.personal</field>
  <field name="type">form</field>

  <field name="arch" type="xml">
    <form string="Accion de personal">
      <group colspan="4" col="4">
	<field name="name" colspan="2"/>
	<field name="date" colspan="2"/>
	<field name="crea_uid" readonly="1" colspan="4"/>
	<field name="contract_id" colspan="4" on_change="onchange_contract_accionp(contract_id)"/>
	<field name="nivel_contrato" colspan="4"/>
	<group colspan="4">
	  <field name="tipo" colspan="2" required="1"/>
	  <field name="date_from" colspan="2" required="1"/>
		<field name="alcalde_e" colspan="2"/>
  	<field name="talento_e" colspan="2"/>
		<field name="subrog_alcalde" colspan="2"/>
  	<field name="subrog_talento" colspan="2"/>
	  <group colspan="4" col='2'>
	    <group string="ANTECEDENTES" colspan="2">
	      <field name="desc" nolabel="1" colspan="2" required="1"/>
	    </group>
	    <group string="EXPLICACION" colspan="2">
	      <field name="desc1" nolabel="1" colspan="2" required="1"/>
	    </group>
	  </group>
	  <field name='si_actual' colspan="2"/>
	  <field name="si_nueva" colspan="2"/>
	  <group colspan="2" string="Situacion Actual" attrs="{'invisible': [('si_actual', '=',True)]}">
	    <field name="direccion_id" colspan="4" attrs="{'required': [('si_actual','!=',True)]}" widget="selection"/>
	    <field name="department_id" colspan="4" attrs="{'required': [('si_actual', '!=',True)]}" widget="selection"/>
	    <field name="cargo_id" colspan="4" attrs="{'required': [('si_actual', '!=',True)]}" widget="selection"/>
	    <field name="nivel_id" colspan="4" attrs="{'required': [('si_actual', '!=',True)]}"/>
	    <field name="nivel_contrato_id" colspan="4" attrs="{'required': [('si_actual', '!=',True)]}"/>
	    <field name="lugar" colspan="4"/>
	    <field name="rmu" colspan="4" attrs="{'required': [('si_actual', '!=',True)]}"/>
	    <field name="budget_id" colspan="4" domain="[('type_budget','=','gasto')]" attrs="{'required': [('si_actual', '!=',True)]}"/>
	  </group>
	  <group colspan="2" string="Situacion Propuesta" attrs="{'invisible': [('si_nueva', '=',True)]}">
	    <field name="direccion_id2" colspan="4" attrs="{'required': [('si_nueva', '!=',True)]}" widget="selection"/>
	    <field name="department_id2" colspan="4" attrs="{'required': [('si_nueva', '!=',True)]}" widget="selection"/>
	    <field name="cargo_id2" colspan="4" attrs="{'required': [('si_nueva', '!=',True)]}" widget="selection"/>
	    <field name="nivel_id2" colspan="4" attrs="{'required': [('si_nueva', '!=',True)]}"/>
	    <field name="nivel_contrato_id2" colspan="4" attrs="{'required': [('si_nueva', '!=',True)]}"/>
	    <field name="lugar2" colspan="4"/>
	    <field name="rmu2" colspan="4" attrs="{'required': [('si_nueva', '!=',True)]}"/>
	    <field name="budget_id2" colspan="4" domain="[('type_budget','=','gasto')]"/>
	  </group>
	</group>
      </group>
      <group colspan="4" col="4">
	<field name="state" readonly="1" widget="statusbar" statusbar_colors='{"auto":"blue","Ejecutado":"blue","Anulado":"grey"}'/>
	<button name="accionPersonalEjecutar" states="Borrador" string="Ejecutar" type="object" icon="gtk-media-play"/>
	<button name="accionPersonalAnular" states="Borrador" string="Anular" type="object" icon="gtk-cancel"/>
      </group>
    </form>
  </field>
</record>

