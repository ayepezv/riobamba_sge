    <record model="ir.ui.view" id="view_employee_modified_tree">
      <field name="name">hr.employee.modified.tree</field>
      <field name="model">hr.employee</field>
      <field name="type">tree</field>
      <field name="inherit_id" ref="hr.view_employee_tree"/>
      <field name="arch" type="xml">
	<xpath expr="/tree/field[@name='name']" position="after">
	  <!--field name="employee_first_lastname"/>
	      <field name="employee_second_lastname"/>
	      <field name="employee_first_name"/>
	      <field name="employee_second_name"/-->
	  <field name="complete_name"/>
	</xpath>
	<xpath expr="/tree/field[@name='work_email']" position="after">
	  <field name="email"/>
	</xpath>
	<xpath expr="/tree/field[@name='company_id']" position="replace"/>
	<xpath expr="/tree/field[@name='parent_id']" position="replace"/>
	<xpath expr="/tree/field[@name='coach_id']" position="replace"/>
	<!--field name="coach_id" position="replace"/>
	    <field name="parent_id" position="replace"/-->
	<!--field name="category_ids" position="replace"/>
            <field name="parent_id" position="replace"/-->
      </field>
    </record>
    
    <record model="ir.ui.view" id="view_employee_modified_filter">
      <field name="name">hr.employee.modified.filter</field>
      <field name="model">hr.employee</field>
      <field name="type">search</field>
		<field name="inherit_id" ref="hr.view_employee_filter"/>
		<field name="arch" type="xml">
			<xpath expr="/search/field[@name='name']" position="before">
				<filter icon="gtk-apply" domain="[('active','=',True)]" string="Activos"/>
				<filter icon="gtk-close" domain="[('active','=',False)]" string="Inactivos"/>
			</xpath>
			<xpath expr="/search/field[@name='name']" position="after">
				<field name="complete_name"/>
			</xpath>
			<xpath expr="/search/field[@name='department_id']" position="replace">
				<field name="department_id"/>
			</xpath>
			<xpath expr="/search/field[@name='job_id']" position="replace">
				<field name="job_id"/>
			</xpath>
			<xpath expr="/search/field[@name='category_ids']" position="replace"/>
			<xpath expr="/search/field[@name='parent_id']" position="replace"/>
			<!--xpath expr="/search/group/filter[@string='Coach']" position="replace"/>
			<xpath expr="/search/group/filter[@string='Coach']" position="replace"/>
			<xpath expr="/search/group/filter[@string='Manager']" position="replace"/>
			<xpath expr="/search/group/filter[@string='Company']" position="replace"/-->
			<xpath expr="/search/group[@string='Group By...']" position="replace">
				<group expand="0" string="Agrupar por...">
                    <filter string="Departamento" icon="terp-personal+" domain="[]" context="{'group_by':'department_id'}"/>
                    <filter string="Cargo" icon="terp-gtk-select-all" domain="[]" context="{'group_by':'job_id'}"/>
                </group>
			</xpath>
		</field>
	  </record>
	  
