            <field name="inherit_id" ref="hr_payroll.hr_salary_rule_list"/>
            <field name="arch" type="xml">
            	<xpath expr="/tree/field[@name='register_id']" position="replace">
            		<field name="sequence"/>
            	</xpath>
            </field>
        </record>

      <record id="view_salary_rule_form_inherit" model="ir.ui.view">
            <field name="name">Salary.Rule.Form</field>
            <field name="model">hr.salary.rule</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="hr_payroll.hr_salary_rule_form"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='appears_on_payslip']" position="after">
            	  <field name="is_replanifica"/>
            	  <field name="agrupar"/>
            	  <field name="is_tercero"/>
            	  <field name="partner_id" attrs="{'required': [('is_tercero', '=',True)]}"/>
		  <field name="account_id"/>
		  <field name="tercero_id"/>
		  <field name="is_ingreso_anticipo"/>
		  <field name="is_rendicion"/>
		  <field name="is_ingreso_gad"/>
            	</xpath>
		<xpath expr="/form/notebook/page[@string='Description']" position="replace">
		  <page string="Fondo Terceros Tipo Rol">
		    <field name="account_ids">
		      <tree editable='bottom'>
			<field name="tipo_id"/>
			<field name="account_id"/>
		      </tree>
		    </field>
		  </page>
		  <page string="CxC">
		    <field name="cxp_ids">
		      <tree editable='bottom'>
			<field name="name"/>
			<field name="account_id"/>
		      </tree>
		    </field>
		  </page>
		</xpath>
            </field>
        </record>
        
        <!--
        =======================
        HR PAYROLL STRUCTURE
        =======================
        -->
        
        <record id="view_payroll_structure_inherit" model="ir.ui.view">
            <field name="name">Payroll.Structure.Form</field>
            <field name="model">hr.payroll.structure</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="hr_payroll.view_hr_employee_grade_form"/>
            <field name="arch" type="xml">
            	<xpath expr="/form/notebook/page/field[@name='rule_ids']/tree/field[@name='register_id']" position="replace">
            		<field name="sequence"/>
            	</xpath>
            </field>
        </record>
        
        
    </data>
</openerp>
