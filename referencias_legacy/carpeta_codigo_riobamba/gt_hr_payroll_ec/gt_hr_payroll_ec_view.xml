	<!-- resumen rol -->
    <record model="ir.ui.view" id="view_resumen_rol_form">
      <field name="name">Resumen Rol</field>    
      <field name="model">resumen.rol</field>
      <field name="type">form</field>
      <field name="arch" type="xml">         
        <form string="Resumen">
          <group  colspan="4">
	    <field name="period_id" colspan="2" required="1"/>
	  </group> 
          <group  colspan="4" col="10">
	    <field name="total_funcionarios" colspan="2" readonly="1"/>
	    <field name="total_rmu" colspan="2" readonly="1"/>
	    <field name="total_ingresos" colspan="2" readonly="1"/>
	    <field name="total_egresos" colspan="2" readonly="1"/>
	    <field name="total" colspan="2" readonly="1"/>
	  </group> 
	  <group  colspan="4">
	    <button name="generarResumenRol" string="Generar" icon="gtk-execute" type="object"/>
	    <button name="printResumenRol" string="Imprimir" icon="gtk-print" type="object"/>
	    <button name="printResumenRolDetalle" string="Imprimir Detalle" icon="gtk-print" type="object"/>
	  </group>
	  <notebook colspan="4">
	    <page string="Detalle Tipo Rol">
	      <field name="line_ids" readonly="1" nolabel="1">
		<tree>
		  <field name="tipo_id"/>
		  <field name="total_funcionarios" sum="total"/>
		  <field name="total_rmu" sum="total"/>
		  <field name="total_ingresos" sum="total"/>
		  <field name="total_egresos" sum="total"/>
		  <field name="total" sum="total"/>
		</tree>
		<form>
		  <field name="tipo_id"/>
		  <field name="total"/>
		</form>
	      </field>
	    </page>
	    <page string="Detalle Rol">
	      <field name="line2_ids" readonly="1" nolabel="1">
		<tree>
		  <field name="contract_id"/>
		  <field name="rmu" sum="total"/>
		  <field name="ingresos" sum="total"/>
		  <field name="egresos" sum="total"/>
		  <field name="total" sum="total"/>
		</tree>
		<form>
		  <field name="contract_id"/>
		  <field name="total"/>
		</form>
	      </field>
	    </page>
	  </notebook>
        </form>
      </field>
    </record>

