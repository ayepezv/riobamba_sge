      <record id="view_print_budget_card" model="ir.ui.view">
        <field name="name">Cedula Presupuestaria</field>
        <field name="model">report.budget.card</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
          <form string="Cédula Presupuestaria">
	    <notebook colspan="4">
	     <page string="Formulario">
            <group colspan="4" string="Cédula Presupuestaria">
	      <field name="fiscalyear" required="1" on_change="onchange_fiscalyear(fiscalyear)"/>
	      <field name="date_from" string='Fecha Inicio' required="1" colspan="2"/>
	      <field name="date_to" string='Fecha Fin' required="1" colspan="2"/>
	      <field name="nivel_aux" string='Nivel AUX' colspan="2" invisible="1"/>
	      <field name="nivel" string='Nivel' colspan="2"/>
	      <field name="project" colspan="" on_change="onchange_project(project,date_from,date_to,context)" 
		     attrs="{'invisible': [('tipo', '=','ingreso')]}"/>
	      <field name="tipo_nivel" string='Tipo Nivel' colspan="2"/>
	      <field name="proy" colspan="4" on_change="onchange_proyecto(proy,date_from,date_to,context)"
		     attrs="{'invisible': [('tipo', '=','ingreso')]}"/>
	      <field name="asig_inicial" colspan=""/>
	      <field name="tipo" invisible="1" colspan="2"/>
	      <field name="project_id" colspan="2" 
		     attrs="{'invisible': [('proy', '==',False)], 'required': [('proy','=',True)]}"/>
             <field name="program_ids" colspan="4" 
		    attrs="{'invisible': [('project', '==',False)], 'required': [('project','=',True)]}"/>
	    </group>
	    <separator string="Archivo Excel" colspan="4"/>
	      <field name="datas_fname" colspan="2" invisible="1"/>
	      <field name="datas" filename="datas_fname" colspan="4" readonly="1"/>
	      <separator string="Archivo Txt" colspan="4"/>
		<field name="filename" invisible="1"/>
		<field name="data" filename="filename" colspan="4"/>
            <separator  colspan="4" />
	    <!--button icon="gtk-cancel" special="cancel" string="Salir" /-->
	    <group colspan="4" col='5'>
	      <button name="print_report" type="object" icon="gtk-print" string="Imprimir"/>
	      <button name="print_report_sobregiradas" attrs="{'invisible':[('tipo','=','ingreso')]}" type="object" icon="gtk-print" string="Imprimir sobregiradas"/>
	      <button name="gen_esigef_file" type="object" icon="gtk-print" string="Genera Archivos ESIGEF y Excel"/>
	      <button name="gen_esigef_pdf" type="object" icon="gtk-print" string="Nivel 4 Esigef"/>
	      <button name="gen_excel_budget" type="object" icon="gtk-print" string="Exporta Excel"/>
	    </group>
	     </page>
	     <page string="Mensajes esigef">
	       <group expand="1">
		 <field name="mensajes" colspan="4" nolabel="1"/>
	       </group>
	     </page>
	     </notebook>
	  </form>
        </field>
      </record> 
      
