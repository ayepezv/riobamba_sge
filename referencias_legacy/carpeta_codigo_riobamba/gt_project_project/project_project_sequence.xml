<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for project.project --> 
        <record id="seq_type_project_project" model="ir.sequence.type">
            <field name="name">Proyectos</field>
            <field name="code">project.project</field>
        </record>
        <record id="seq_project_project" model="ir.sequence">
            <field name="name">Project Project</field>
            <field name="code">project.project</field>
            <field name="prefix">PP-GPA/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_type_project_transfer" model="ir.sequence.type">
            <field name="name">Transferencia de Fondos de Proyectos</field>
            <field name="code">project.budget.transfer</field>
        </record>
        <record id="seq_project_transfer" model="ir.sequence">
            <field name="name">Fondos de Proyectos</field>
            <field name="code">project.budget.transfer</field>
            <field name="prefix">TFP</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
