<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>


      <record id="return_start_form" model="ir.ui.view">
        <field name="name">return.start.form</field>
        <field name="model">return.start</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
          <form>
			<field name="name" required="1" colspan="4"/>
            <separator string="" colspan="4" />
            <label string="" colspan="2"/>
            <group col="2" colspan="2">
              <button icon='gtk-cancel' special="cancel" string="Cancelar" />
              <button name="create_incident_start" string="Regresar" confirm="Esta seguro que desea devolver al inicio???" colspan="1" type="object" icon="gtk-go-back" />
            </group>
          </form>
        </field>
      </record>
	  
      <record id="action_return_start" model="ir.actions.act_window">
        <field name="name">Regresar Inicio</field>
        <field name="res_model">return.start</field>
        <field name="view_type">form</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
      </record>
	  
    </data>
</openerp>
