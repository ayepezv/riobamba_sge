<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for doc_contract --> 
        <record id="doc_contract_contract" model="ir.sequence.type">
            <field name="name">Doc Contract</field>
            <field name="code">seq.doc.contract</field>
        </record>
        <record id="seq_doc_contract_contract" model="ir.sequence">
            <field name="name">Doc Contract Contract</field>
            <field name="code">seq.doc.contract</field>
            <field name="suffix">-CT-%(month)s-%(year)s</field>
            <field name="padding">4</field>
        </record>

	<record id="doc_contract_state" model="ir.sequence.type">
            <field name="name">Estados Etapas de Contratos</field>
            <field name="code">seq.doc.contract.state</field>
        </record>
        <record id="seq_state" model="ir.sequence">
            <field name="name">Estados Etapas Contratos</field>
            <field name="code">seq.doc.contract.state</field>
            <field name="padding">3</field>
        </record>
    </data>
</openerp>
